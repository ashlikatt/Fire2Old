let objectMemory: BigArray<Any> = BigArray::new();
let memID: Int = 0;

struct Box<T> {
  let pointer: Int;
  
  fn from<R>(val: R): Box<R> {
    memID += 1;
    objectMemory[pointer] = val;
    return Box<R> {
      pointer: memID
    }
  }
  
  fn deref(self): T {
    return objectMemory[self.pointer] as T;
  }
}
